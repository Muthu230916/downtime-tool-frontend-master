		"@fullcalendar/core": "^4.3.1",
		"@fullcalendar/resource-timeline": "^4.3.0",
		"@movilitas/mov-base": "^1.2.1",
		"@swimlane/ngx-charts": "^12.0.1",
		"angular-animations": "0.0.10",
		"angular-font-awesome": "^3.1.2",
		"angular-tree-component": "^8.4.0",
		"angularx-flatpickr": "^6.1.1",
		"core-js": "^3.2.1",
