{
	"name": "downtimes",
	"version": "1.0.2",
	"scripts": {
		"start": "ng serve --proxy-config proxy.conf.json",
		"start-fresh-lib": "npm run build-lib && ng serve --proxy-config proxy.conf.json",
		"tests": "ng test",
		"copy-assets-lib": "copyfiles -u 3 ./projects/mov-downtimes/src/styles.css ./dist/mov-downtimes",
		"start-with-assets": "npm run copy-assets-lib && ng serve --proxy-config proxy.conf.json",
		"build-lib": "ng build mov-downtimes && npm run copy-assets-lib",
		"watch-lib": "ng build mov-downtimes --watch",
		"build-app-with-lib": "npm run build-lib && npm run build-app",
		"build-app": "ng build --prod --outputHashing=all --base-href index.html"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "^8.2.14",
